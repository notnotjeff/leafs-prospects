{
  "name": "client",
  "jest": {
    "restoreMocks": true,
    "clearMocks": true,
    "collectCoverageFrom": [
      "**/src/**",
      "!**/node_modules/**",
      "!src/constants/**",
      "!test/**"
    ]
  },
  "version": "2.0.0",
  "private": true,
  "homepage": "https://leafs-prospects.com",
  "engines": {
    "node": "8.11.3",
    "yarn": "1.9.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --ignore-path ./.gitignore .",
    "prettier": "prettier \"**/*.+(js|json)\" --ignore-path ./.prettierignore",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:no-watch": "react-scripts test --env=jsdom --watchAll=false",
    "validate": "npm-run-all --parallel check-format lint test:no-watch build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "faker": "^4.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-query": "^1.5.8",
    "react-query-devtools": "^1.1.17",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.3.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "jest-axe": "^3.4.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
